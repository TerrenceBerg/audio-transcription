{
    "name": "terrence-christopher/audio-transcription",
    "description": "A Laravel package for audio transcription. Audio to Text Transcriber",
    "license": "MIT",
    "authors": [
        {
            "name": "Terrence Christopher",
            "email": "terrencechristopher28@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "TerrenceChristopher\\AudioTranscription\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "TerrenceChristopher\\AudioTranscription\\src\\Providers\\AudioTranscriptionServiceProvider"
            ],
            "aliases": {
                "AudioTranscription": "TerrenceChristopher\\AudioTranscription\\src\\Facades\\AudioTranscriptionFacade"
            }
        }
    },
    "require": {
        "php": "^8.1",
        "laravel/framework": "^12.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
